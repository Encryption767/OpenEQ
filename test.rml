<rml>
	<head>
		<title>OpenEQ Demo!</title>
		<link type="text/template" href="window.rml" />
		<style>
			body
			{
				width: 500px;
				height: 400px;
				
				margin: auto;
			}
			
			div#title_bar div#icon
			{
				display: none;
			}
			
			div#content
			{
				text-align: left;
			}
		</style>
	</head>
	<body template="window">
		<script>
			function test()
				print 'test!'
			end
			function test2(self, e)
				print 'wooooooo'
				print(self)
				print(e)
			end
			button = document.getElementById('testbutton')
			button.inner_rml = 'Bar'
			button.bind('click', test)
		</script>
		This is a sample.<br>
		<img src="transtest.png" width="50" height="50"><br>
		Hallo from OpenEQ!<br>
		<button id="testbutton" onclick="test2">Foo</button><br>
		<input id="testinput" type="text" value="foo" size="10"><br>
	</body>
</rml>
